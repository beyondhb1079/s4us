    "dev": "vite",
    "build": "vite build",
    "start": "scripts/emulators_exec.sh 'vite'",
    "test": "scripts/emulators_exec.sh 'vitest'",
    "test:run": "scripts/emulators_exec.sh 'vitest run'",
    "ci-test": "scripts/emulators_exec.sh 'vitest --coverage'",
    "serve": "vite preview",
    "lint": "eslint --cache src/ --ext .js,.jsx,.ts,.tsx",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "./src/": {
        "statements": 60,
        "branches": 65,
        "functions": 50,
        "lines": 60
      },
      "./src/components/": {
        "statements": 45,
        "branches": 50,
        "functions": 35,
        "lines": 50
      },
      "./src/lib/": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      },
      "./src/models/**/*.ts": {
        "statements": 90,
        "branches": 80,
        "functions": 85,
        "lines": 90
      },
      "./src/pages/": {
        "statements": 40,
        "branches": 45,
        "functions": 20,
        "lines": 35
      }
    }
  },
  "devDependencies": {
