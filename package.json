      },
      "./src/lib/": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      },
      "./src/models/**/*.ts": {
        "statements": 90,
        "branches": 80,
        "functions": 85,
        "lines": 90
      },
      "./src/pages/": {
        "statements": 40,
        "branches": 45,
        "functions": 20,
        "lines": 35
      }
    }
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.0",
